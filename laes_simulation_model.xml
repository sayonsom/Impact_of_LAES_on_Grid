<?xml version="1.0" encoding="utf-8"?>
<griddyn name="laes_power_model" version="0.1.0">
   <!-- This is where the large generator is connected, also the slack bus -->
   <bus name="bus1">
      <type>SLK</type>
      <angle>0</angle>
      <voltage>1.05</voltage>
      <generator name="sync_gen">
      <dynmodel>typical</dynmodel>
      <pmax>4</pmax>
      </generator>
      <load name="gen_plant_loads">
         <P>1.15</P>
         <Q>0.31</Q>
         <event>@1|p=1.1</event>
      </load>
   </bus>

   <!-- The load bus -->
   <bus name="bus2">
      <type>PQ</type>
      <load name="all_loads">
         <P>0.45</P>
         <Q>0.2</Q>
      </load>
   </bus>

   <!-- The integration bus, i.e. where the RES and the LAES merge -->
   <bus name="bus3">
      <type>PV</type>
      <load name="merge_bus">
         <P>0</P>
         <Q>0</Q>
      </load>
   </bus>

   <!-- LAES Bus, also has a generator -->
   <bus name="laes_bus">
      <type>PV</type>
      <angle>0</angle>
      <voltage>1.05</voltage>
      <generator name="gen1">
         <!-- TODO: The dynamic model is to be changed for LAES, up for discussion-->
         <model>
            <type>fourthOrder</type>
            <D>0.040</D>
            <H>5</H>
            <Tdop>8</Tdop>
            <Tqop>1</Tqop>
            <Xd>1.050</Xd>
            <Xdp>0.350</Xdp>
            <Xq>0.850</Xq>
            <Xqp>0.350</Xqp>
         </model>
         <exciter>
            <type>basic</type>
            <Aex>0</Aex>
            <Bex>0</Bex>
            <Ka>20</Ka>
            <Ke>1</Ke>
            <Kf>0.040</Kf>
            <Ta>0.200</Ta>
            <Te>0.700</Te>
            <Tf>1</Tf>
            <Urmax>50</Urmax>
            <Urmin>-50</Urmin>
         </exciter>
         <governor>
            <type>basic</type>
            <K>16.667</K>
            <Pdown>0</Pdown>
            <Pmax>0</Pmax>
            <Pmin>0</Pmin>
            <Pup>0</Pup>
            <T1>0.100</T1>
            <T2>0.150</T2>
            <T3>0.050</T3>
         </governor>
      </generator>
   </bus>

   <!-- Renewable Energy Bus -->
   <bus name="bus5">
      <type>PQ</type>
      <load name="res_bus">
         <P>0</P>
         <Q>0</Q>

         <event>
             <field>P</field>
             <value>1.3</value>
             <time>1</time>
         </event>
       
         <event>
             <field>P</field>
             <value>1.5</value>
             <time>3</time>
         </event>
      </load>
   </bus>

   <link from="bus1" name="line_12" to="bus2">
      <b>0.1273164</b>
      <r>0.083931984</r>
      <x>0.518336712</x>
   </link>

   <link from="bus1" name="line_13" to="bus3">
      <b>0.1273164</b>
      <r>0.083931984</r>
      <x>0.518336712</x>
   </link>

   <link from="bus2" name="line_23" to="bus3">
      <b>0.1273164</b>
      <r>0.083931984</r>
      <x>0.518336712</x>
   </link>

   <link from="bus5" name="line_53" to="bus3">
      <b>0.1273164</b>
      <r>0.083931984</r>
      <x>0.518336712</x>
   </link>

   <link from="bus4" name="line_43" to="bus3">
      <b>0.1273164</b>
      <r>0.083931984</r>
      <x>0.518336712</x>
   </link>

   <timestop>10</timestop>
   <recorder period=0.05 field="auto">
   <file>laes_simulation_results.csv</file>
   </recorder>
</griddyn>